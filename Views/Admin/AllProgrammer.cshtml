@model IEnumerable<IITS_Programming_Club.Models.Programmer>
@using Microsoft.AspNetCore.Http;
@using IITS_Programming_Club.Models;

    <div class="container">
        @{
            string s = Context.Session.GetString("adminid");
            if (s == null)
            {
                @:Please login First
            }
                else
                {
        <table class="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.week)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.id)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.isDone)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.week)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.id)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.isDone)
                        </td>
                        <td>
                            <a asp-controller="Admin" asp-action="ProgrammerEdit" asp-route-id="@item.key">Edit</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
                }
            }
                

            </div>